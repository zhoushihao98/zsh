.txt {
  font-weight: bold;
}

.arrow{
  width: 12px;
  height: 12px;
  border-top: 2px solid rgba(128, 128, 128, 0.726);
  border-right: 2px solid rgba(128, 128, 128, 0.726);
  transform: rotate(45deg);
}
.arrow-view{
  width: 50%;
}
.rounds {
	border-radius: 30rpx;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}
